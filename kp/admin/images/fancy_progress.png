					data.aspectratio = obj.aspectratio;
					
					// SYNC AJAX REQUEST
					jQuery.ajax({
						type:"post",
						url:"http://thestreettimes.com/wp-admin/admin-ajax.php",
						dataType: 'json',
						data:data,
						async:false,
